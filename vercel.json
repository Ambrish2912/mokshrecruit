{
  "buildCommand": "cd my-app && npm run build",
  "outputDirectory": "my-app/dist",
  "installCommand": "npm install --prefix ./my-app --legacy-peer-deps",
  "devCommand": "cd my-app && npm run dev",
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}
